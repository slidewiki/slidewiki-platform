{
  "name": "slidewiki-platform",
  "description": "A Platform for Collaborative OpenCourseWare Authoring",
  "keywords": [
    "E-Learinng",
    "OpenCourseWare",
    "Presentations",
    "Authoring"
  ],
  "author": "Ali Khalili(http://ali1k.com)",
  "license": "MPL-2.0",
  "version": "2.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/slidewiki/slidewiki-platform"
  },
  "bugs": "https://github.com/slidewiki/slidewiki-platform/issues",
  "engines": {
    "node": ">=6.11.0"
  },
  "main": "start.js",
  "scripts": {
    "install": "napa",
    "clean": "rm -rf build/ coverage/",
    "start": "webpack --config ./webpack/prod.settings.config.js && NODE_ENV=production node start.js",
    "start:windows": "webpack --config ./webpack/prod.settings.config.js && set NODE_ENV=production && node start.js",
    "build": "webpack --config ./webpack/prod.config.js && NODE_ENV=production npm start",
    "build:windows": "webpack --config ./webpack/prod.config.js && set NODE_ENV=production && set BABEL_ENV=production && npm start",
    "build:nostart": "webpack --config ./webpack/prod.config.js",
    "dev": "node ./webpack/dev-server.js",
    "dev:windows": "set HOST=127.0.0.1&& set PORT=3000&& node ./webpack/dev-server.js",
    "langs": "NODE_ENV=development BABEL_ENV=development babel plugins/intl/translate.js | node",
    "langs:windows": "set NODE_ENV=development && set BABEL_ENV=development && babel plugins/intl/translate.js | node",
    "lint": "eslint -c .eslintrc \"./**/*.js\"",
    "test:components": "mocha test/setup.js test/components/**/*.js",
    "test:unit": "mocha test/setup.js test/unit/**/*.js",
    "coverage": "istanbul cover _mocha -- --require @babel/register test/setup.js test/components/*.js",
    "coverall": "npm run coverage && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "countLOC": "sloc -f cli-table -k total,source,comment,empty -e node_modules\\|coverage\\|.git\\|bower_components\\|custom_modules ./",
    "countLOC:details": "sloc -f cli-table -d -e node_modules\\|coverage\\|.git\\|bower_components\\|custom_modules ./",
    "start:watch": "nodemon",
    "validate": "npm ls",
    "pull": "git pull --recurse-submodules",
    "precommit": "npm run lint",
    "prelangs": "rm -rf ./build/messages/ && webpack --config ./webpack/langs.config.js",
    "windows": "webpack --config ./webpack/prod.config.js && set NODE_ENV=production && npm start"
  },
  "napa": {
    "ckeditor-lineheight-plugin": "https://github.com/lotusgodkk/lineheight.git",
